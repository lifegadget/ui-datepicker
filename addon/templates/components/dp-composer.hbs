{{ui-animate animateOut domElement=elementId duration=0.5 event='exit'}}
<dp-composer id={{elementId}} class="{{class}} fill {{skin}}">
  {{#each _inputs as |i|}}
    {{component (concat 'dp-input-' i)
      datetime=datetime
      onChange=(action onChange)
    }}
  {{/each}}
</dp-composer>
{{yield}}
