<years class="dp-calendar-years flex fill">
  {{#if hasBlock}}
    {{yield}}
  {{else}}

    {{ui-knob
      min=minDecade
      max=maxDecade
      value=1950
      step=1
      thickness=0.3
      angleOffset=-125
      angleArc=250
      cursor=5
      skin='tron'
      width='250'
      height='250'
    }}

  {{/if}}
</years>
